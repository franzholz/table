{
  "name": "jambagecom/table",
  "type": "typo3-cms-extension",
  "description": "Table Library for TYPO3 CMS",
  "keywords": [
    "TYPO3",
    "extension",
    "library"
  ],
  "support": {
    "issues": "https://github.com/franzholz/table/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5 || ^12.4",
    "jambagecom/div2007": "^1.15 || ^2.0"
  },
  "suggest": {
    "friendsoftypo3/typo3db-legacy": "^1"
  },
  "autoload": {
    "classmap": [
      "lib"
    ]
  },
  "replace": {
    "typo3-ter/table": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "table"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  },
  "require-dev": {
    "ssch/typo3-rector": "^1.5",
    "friendsofphp/php-cs-fixer": "^3.43"
  }
}
